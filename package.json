{
    "name": "Vertical",
    "version": "0.0.1",
    "description": "Adds keybindings to allow tiling windows to the top or bottom of a screen. Similar to the built-in left and right. Particually useful for portrait monitors",
    "main": "extension.js",
    "sourceType": "module",
    "scripts": {
        "test": "prettier --list-different \"./**/*.{js,jsx,ts,tsx,json}\"",
        "prepare": "husky install",
        "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ross/vertical.git"
    },
    "keywords": [
        "gnome",
        "vertical",
        "portrait",
        "tiling",
        "javascript",
        "gnome-shell"
    ],
    "author": "Ross McFarland",
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/ross/vertical/issues"
    },
    "homepage": "https://github.com/ross/vertical",
    "devDependencies": {
        "husky": "^8.0.1",
        "prettier": "^2.7.1"
    }
}
